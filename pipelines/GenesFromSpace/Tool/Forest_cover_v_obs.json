  "inputs": {
    "pipeline@41": {
      "label": "Size of buffer",
      "description": "Radius size [in km] to determine population presence around the coordinates of species observations.",
      "type": "float",
      "example": 10,
      "weight": 0
    },
    "pipeline@44": {
      "label": "Years of interest",
      "description": "List of years for which tree cover should be extracted (maximum range 2000 - 2023).",
      "type": "int[]",
      "example": [
        2000,
        2005,
        2010,
        2015,
        2020
      ],
      "weight": 1
    },
    "pipeline@42": {
      "label": "Distance between populations",
      "description": "Distance [in km] to separate species observations in different populations.",
      "type": "float",
      "example": 50,
      "weight": 2
    },
    "pipeline@45": {
      "label": "Ne:Nc ratio estimate",
      "description": "Estimated Ne:Nc ratio for the studied species. Multiple values can be provided, separated by a comma.",
      "type": "float[]",
      "example": [
        0.1,
        0.2
      ],
      "weight": 3
    },
    "pipeline@47": {
      "label": "Population density",
      "description": "Estimated density of the population [number of individuals per km2]. Multiple values can be provided, separated by a comma.",
      "type": "float[]",
      "example": [
        50,
        100,
        1000
      ],
      "weight": 4
    },
    "pipeline@46": {
      "label": "Title of the run",
      "description": "Set a name for the pipeline run.",
      "type": "text",
      "example": "Quercus sartorii, Mexico, Habitat decline by tree cover loss, 2000-2023",
      "weight": 5
    },
    "GenesFromSpace>ToolComponents>GetPopulationPolygons>GFS_Population_polygons_from_table_observations.json@39|GFS_IndicatorsTool>get_pop_poly.yml@5|species_obs": {
      "description": "Path to the table storing the species observation coordinates. The table must incude header with \"decimal_longitude\" and \"decimal_latitude\" columns, indicating the coordinates of every observation.",
      "label": "Coordinates of species occurrence",
      "weight": 6,
      "type": "text/tab-separated-values",
      "example": "/userdata/obs_data.tsv"
    },
    "pipeline@52": {
      "label": "Username",
      "description": "The username of the person conducting the analysis.",
      "type": "text",
      "example": "johndoe",
      "weight": 7
    },
    "pipeline@53": {
      "label": "User Email",
      "description": "The email address of the person conducting the analysis.",
      "type": "text",
      "example": "johndoe@example.com",
      "weight": 8
    }
  },
  "outputs": {
    "GenesFromSpace>ToolComponents>GetIndicators>GFS_Indicators.json@32|GFS_IndicatorsTool>get_Indicators.yml@162|pm": {
      "description": "Estimated proportion of mantained populations, comparing earliest and latest time point. A value of 1 means that no populations went extinct over the time frame.",
      "label": "Population maintained indicator",
      "weight": 0,
      "type": "float"
    },
    "GenesFromSpace>ToolComponents>GetIndicators>GFS_Indicators.json@32|GFS_IndicatorsTool>get_Indicators.yml@162|ne500": {
      "description": "Estimated proportion of populations with Ne>500 at latest time point.",
      "label": "Ne>500 indicator",
      "weight": 1,
      "type": "float"
    },
    "GenesFromSpace>ToolComponents>GetIndicators>GFS_Indicators.json@32|GFS_IndicatorsTool>get_Indicators.yml@162|ne_table": {
