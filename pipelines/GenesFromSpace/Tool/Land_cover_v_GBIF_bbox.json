  "inputs": {
    "pipeline@187": {
      "label": "Species names",
      "description": "Scientific name of the species, used to look for occurrences in GBIF. ",
      "type": "text",
      "example": "Quercus sartorii",
      "weight": 0
    },
    "pipeline@185": {
      "label": "Years of interest",
      "description": "List of years for which landcover should be extracted (maximum range 1992 - 2020).",
      "type": "int[]",
      "example": [
        1995,
        2000,
        2005,
        2010,
        2015,
        2020
      ],
      "weight": 1
    },
    "pipeline@188": {
      "label": "Bounding box ",
      "description": "Vector of float, bbox coordinates of the bbox in the order xmin, ymin, xmax, ymax",
      "type": "float[]",
      "example": [
        "-99",
        "22",
        "-92",
        "29"
      ],
      "weight": 2
    },
    "pipeline@186": {
      "label": "Landcover classes",
      "description": "List of landcover class identifiers to be extract (for identifiers see https://savs.eumetsat.int/html/images/landcover_legend.png)",
      "type": "int[]",
      "example": [
        130,
        140
      ],
      "weight": 3
    },
    "pipeline@192": {
      "label": "Start year",
      "description": "Integer, 4 digit year, start date to retrieve occurrences",
      "type": "int",
      "example": 1980,
      "weight": 4
    },
    "pipeline@193": {
      "label": "End year",
      "description": "Integer, 4 digit year, end date to retrieve occurrences",
      "type": "int",
      "example": 2000,
      "weight": 5
    },
    "pipeline@190": {
      "label": "Size of buffer",
      "description": "Radius size [in km] to determine population presence around the coordinates of species observations.",
      "type": "float",
      "example": 10,
      "weight": 6
    },
    "pipeline@191": {
      "label": "Distance between populations",
      "description": "Distance [in km] to separate species observations in different populations.",
      "type": "float",
      "example": 50,
      "weight": 7
    },
    "pipeline@194": {
      "label": "Username",
      "description": "The username of the person conducting the analysis.",
      "type": "text",
      "example": "johndoe",
      "weight": 8
    },
    "pipeline@195": {
      "label": "User Email",
      "description": "The email address of the person conducting the analysis.",
      "type": "text",
      "example": "johndoe@example.com",
      "weight": 9
    },
    "pipeline@198": {
      "label": "Ne:Nc ratio estimate",
      "description": "Estimated Ne:Nc ratio for the studied species. Multiple values can be provided, separated by a comma.",
      "type": "float[]",
      "example": [
        0.1,
        0.2
      ],
      "weight": 10
    },
    "pipeline@200": {
      "label": "Population density",
      "description": "Estimated density of the population [number of individuals per km2]. Multiple values can be provided, separated by a comma.",
      "type": "float[]",
      "example": [
        50,
        100,
        1000
      ],
      "weight": 11
    },
    "pipeline@199": {
      "label": "Title of the run",
      "description": "Set a name for the pipeline run.",
      "type": "text",
      "example": "Quercus sartorii, Mexico, Habitat decline by tree cover loss, 2000-2023",
      "weight": 12
    }
  },
  "outputs": {
