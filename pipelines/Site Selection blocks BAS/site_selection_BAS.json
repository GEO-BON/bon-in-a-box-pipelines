    "pipeline@5": {
      "label": "number of columns",
      "description": "Number of columns for the block grid",
      "type": "int",
      "example": 10,
      "weight": 7
    },
    "data>loadFromStac.yml@2|aggregation": {
      "description": "Method used to aggregate items that overlay each other",
