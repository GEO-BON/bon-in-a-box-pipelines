      "type": "options",
      "options": [
        "near",
        "bilinear",
        "average",
        "mode",
        "cubic",
        "cubicspline",
        "lanczos",
        "rms",
        "min",
        "max",
        "sum",
        "med",
        "q1",
        "q3"
      ],
      "example": "near",
      "weight": 3
    },
    "data>loadFromStac.yml@2|mask": {
      "description": "Shapefile, used to mask the output rasters",
      "label": "mask",
      "type": "application/dbf",
      "weight": 4,
