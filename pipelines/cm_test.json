  "inputs": {
    "climateMetrics>loadChelsaProjections.yml@30|rcp": {
      "description": "shared socioeconomic pathways (see https://en.wikipedia.org/wiki/Shared_Socioeconomic_Pathways)",
      "label": "rcp",
      "type": "options",
      "options": [
        "ssp126",
        "ssp370",
        "ssp585"
      ],
      "example": "ssp126",
      "weight": 0
    },
    "climateMetrics>loadChelsaProjections.yml@30|variable": {
      "description": "Chelsa climte variable for projection. See a list of variables [here](https://chelsa-climate.org/wp-admin/download-page/CHELSA_tech_specification.pdf) in section 5.",
      "label": "Climate variable",
      "type": "text",
      "example": "bio1",
      "weight": 1
    },
    "climateMetrics>loadChelsaProjections.yml@30|time_span": {
      "description": "period for climate projection, start year to end year",
      "label": "future time span",
      "type": "options",
      "options": [
        "2011-2040",
        "2041-2070",
        "2071-2100"
      ],
      "example": "2041-2070",
      "weight": 2
    },
    "climateMetrics>loadChelsaMonthly.yml@31|t0": {
      "description": "Current climate start time, in format aaaa-mm-dd",
      "label": "current climate start",
      "type": "text",
      "example": "1981-01-01",
      "weight": 3
    },
    "climateMetrics>loadChelsaMonthly.yml@31|t1": {
      "description": "Current climate end time, in format aaaa-mm-dd",
      "label": "current climate end",
      "type": "text",
      "example": "1981-12-31",
      "weight": 4
    },
    "climateMetrics>climateMetrics.yml@34|metric": {
      "description": "Choice of local velocity (meters/year; see Loarie et. al. (2009) and Sandel et. al. (2011).), forward or backward velocity (meters/year; see Carroll et. al. (2015) and Hamann et. al. (2015).) Or climate rarity",
      "label": "Climate metric",
      "type": "options",
      "options": [
        "local",
        "forward",
        "backward",
        "rarity"
      ],
      "example": "local",
      "weight": 5
    },
    "climateMetrics>climateMetrics.yml@34|moving_window": {
      "description": "Number of cells (must be an odd number) for which do include in moving window. \nOnly appicable to climate rarity.\n",
      "label": "search moving window",
      "type": "int",
      "example": 11,
      "weight": 6
    },
    "climateMetrics>climateMetrics.yml@34|t_match": {
      "description": "Acceptable difference of climate (e.g., mean temperature in Â°C) to consider that the climate is similar.",
      "label": "threshold climate match",
      "type": "float",
      "example": 1,
      "weight": 7
    },
    "pipeline@36": {
      "label": "spatial resolution output",
      "description": "pixel size (in meters) of the output raster",
      "type": "int",
      "example": 1000,
      "weight": 8
    },
    "pipeline@37": {
      "label": "spatial reference output",
      "description": "target spatial reference system of the output raster",
      "type": "text",
      "example": "EPSG:6624",
      "weight": 9
    },
    "pipeline@38": {
      "label": "aggregation method",
      "description": "method of aggregation for the temperature data",
      "type": "options",
      "example": "mean",
      "options": [
        "min",
        "max",
        "mean",
        "median",
        "first"
      ],
      "weight": 10
    },
    "data>getBoundingBox.yml@42|country": {
      "description": "Country of interest. For list of countries look here https://geoio.biodiversite-quebec.ca/docs#/default/validate_country_name_validate_country_name_get.",
      "label": "Country",
      "type": "text",
      "example": "Canada",
      "weight": 11
    },
    "data>getBoundingBox.yml@42|state": {
      "description": "State or province of interest (leave blank if you want the full country). For list of accepted region names look here https://geoio.biodiversite-quebec.ca/docs#/default/validate_state_name_validate_state_name_get.",
      "label": "State/Province",
      "type": "text",
      "example": "Quebec",
      "weight": 12
