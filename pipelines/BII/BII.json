      ],
      "example": "2020",
      "weight": 1
    },
    "data>getBoundingBox.yml@12|state": {
      "description": "State or province of interest (leave blank if you want the full country). For list of accepted region names look here https://geoio.biodiversite-quebec.ca/docs#/default/validate_state_name_validate_state_name_get.",
      "label": "State/Province",
      "type": "text",
      "example": "Quebec",
      "weight": 2
    },
    "data>getBoundingBox.yml@12|study_area_file": {
      "description": "Upload a file of a custom study area",
      "label": "File for study area",
      "type": "text",
      "example": null,
      "weight": 3
    },
    "data>getBoundingBox.yml@12|country": {
      "description": "Country of interest. For list of countries look here https://geoio.biodiversite-quebec.ca/docs#/default/validate_country_name_validate_country_name_get.",
      "label": "Country",
      "type": "text",
      "example": "Canada",
      "weight": 4
    },
    "zonal_statistics>zonal_stats.yml@25|summary_statistic": {
      "description": "Summary statistic for layers",
      "label": "Summary statistic",
      "type": "options[]",
      "options": [
        "mean",
        "median",
        "sum",
        "min",
        "max",
        "stdev",
        "variance",
        "mode"
      ],
      "example": [
        "mean"
      ],
      "weight": 5
    },
    "pipeline@31": {
      "label": "Coordinate reference system",
      "description": "Numerical value referring to the EPSG code (European Petroleum Survey Group) associated with the spatial reference system that will be used as a reference for the study area.\nThis numerical value specifies the projection and geodetic datum used to define the coordinates and spatial representation of the data in the study area.\nThis MUST be in a projected reference system (not lat long).\n\nFor further information on coordinate systems and EPSG codes, you can access the official database on the EPSG website at [https://epsg.org/home.html](https://epsg.org/home.html).\nThe website provides documentation, resources, and tools for searching and understanding the EPSG codes used in various geospatial contexts.\n\n*Leave blank if you want the bounding box to be in EPSG:4326.*\n",
      "type": "text",
      "example": "EPSG:6622",
      "weight": 6
    },
    "data>loadFromStac.yml@18|spatial_res": {
      "description": "Integer, spatial resolution of the rasters (in meters)",
      "label": "Spatial resolution",
      "type": "float",
      "example": 1000,
      "weight": 7
    }
  },
