  "inputs": {
    "pipeline@121": {
      "label": "Species",
      "description": "Name of species",
      "type": "text[]",
      "example": [
        "Acer saccharum"
      ],
      "weight": 0
    },
    "pipeline@137": {
      "label": "Environmental Predictors",
      "description": "Vector of strings, collection name followed by '|' followed by item id",
      "type": "text[]",
      "example": [
        "chelsa-clim|bio1",
        "chelsa-clim|bio3",
        "chelsa-clim|bio4",
        "chelsa-clim|bio12",
        "chelsa-clim|bio15"
      ],
      "weight": 1
    },
    "pipeline@58": {
      "label": "Bounding Box",
      "description": "Vector of floating point numbers. Represent the bounding box in the order xmin, ymin, xmax, ymax. Must be in the same CRS as the CRS input.",
      "type": "float[]",
      "example": [
        -2316297,
        -1971146,
        1015207,
        1511916
      ],
      "weight": 2
    },
    "pipeline@57": {
      "label": "Coordinate Reference System",
      "description": "String, Projection System as an EPSG code.",
      "type": "text",
      "example": "EPSG:6622",
      "weight": 3
    },
    "data>pyLoadObservations>pyLoadObservations.yml@96|data_source": {
      "description": "Source of the data (One of gbif_pc - Planetary computer or gbif_api - GBIF Download API)",
      "label": "Data source",
      "type": "options",
      "options": [
        "gbif_pc",
        "gbif_api"
      ],
      "example": "gbif_api",
      "weight": 4
    },
    "data>pyLoadObservations>pyLoadObservations.yml@96|min_year": {
      "description": "Earliest year of occurrence records",
      "label": "Start Year",
      "type": "int",
      "example": 2010,
      "weight": 5
    },
    "data>pyLoadObservations>pyLoadObservations.yml@96|max_year": {
      "description": "Latest year of occurrence records ",
      "label": "End Year",
      "type": "int",
      "example": 2023,
      "weight": 6
    },
    "pipeline@146": {
      "label": "STAC URL",
      "description": "URL of the Stac Catalog to pull environmental predictors and water mask from.",
      "type": "text",
      "example": "https://stac.geobon.org/",
      "weight": 7
    },
    "pipeline@152": {
      "label": "Pseudoabsence Buffer",
      "description": "The minimum distance a PA is allowed to be from a presence in kilometers",
      "type": "float",
      "example": 10,
      "weight": 8
    },
    "pipeline@153": {
      "label": "Max Candidate Pseudoabsences",
      "description": "The maximum number of candidate pseudoabsences to consider. This speeds up PA generation on large rasters.",
      "type": "int",
      "example": 100000,
      "weight": 9
    },
    "pipeline@154": {
      "label": "Pseudoabsence proportion",
      "description": "The number of PAs, given by the proportion of the total occurrences to use.",
      "type": "float",
      "example": 2.4,
      "weight": 10
    },
    "data>loadFromStac.yml@159|bbox": {
      "description": "Vector of float, bbox coordinates of the extent in the order xmin, ymin, xmax, ymax",
      "label": "Bounding box",
      "type": "float[]",
      "example": [
        -2316297,
        -1971146,
        1015207,
        1511916
      ],
      "weight": 11
    },
    "pipeline@163": {
